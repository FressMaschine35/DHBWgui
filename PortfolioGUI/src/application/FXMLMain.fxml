<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FXMLMainController">
	<center>
		<TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
			<tabs>
				<Tab text="Menü">
					<content>
						<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
					</content>
				</Tab>
				<Tab text="Meine Gärten">
					<content>
						<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
							<children>
								<GridPane AnchorPane.bottomAnchor="4.0" AnchorPane.leftAnchor="1.0" AnchorPane.rightAnchor="1.0" AnchorPane.topAnchor="1.0">
									<columnConstraints>
										<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
									</columnConstraints>
									<rowConstraints>
										<RowConstraints maxHeight="319.0" minHeight="54.0" prefHeight="54.0" vgrow="NEVER" />
										<RowConstraints maxHeight="606.0" minHeight="10.0" prefHeight="606.0" vgrow="SOMETIMES" />
									</rowConstraints>
									<children>
										<Label fx:id="rLabel" text="Meine gepachteten Felder" GridPane.hgrow="NEVER" GridPane.vgrow="NEVER">
											<font>
												<Font size="24.0" />
											</font>
										</Label>
										<TableView fx:id="rentedFields" prefHeight="295.0" prefWidth="600.0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS">
											<columns>
												<TableColumn fx:id="rFieldName" prefWidth="75.0" text="Feldname" />
												<TableColumn fx:id="rStartDate" prefWidth="75.0" text="Von" />
												<TableColumn fx:id="rEndDate" prefWidth="75.0" text="Bis" />
												<TableColumn fx:id="rDetails" prefWidth="75.0" text="Details" />
											</columns>
											<columnResizePolicy>
												<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
											</columnResizePolicy>
										</TableView>
									</children>
								</GridPane>
							</children>
						</AnchorPane>
					</content>
				</Tab>
				<Tab text="Verpachtung">
					<content>
						<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
							<children>
								<GridPane AnchorPane.bottomAnchor="4.0" AnchorPane.leftAnchor="1.0" AnchorPane.rightAnchor="1.0" AnchorPane.topAnchor="1.0">
									<columnConstraints>
										<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
									</columnConstraints>
									<rowConstraints>
										<RowConstraints maxHeight="319.0" minHeight="54.0" prefHeight="54.0" vgrow="NEVER" />
										<RowConstraints maxHeight="606.0" minHeight="10.0" prefHeight="606.0" vgrow="SOMETIMES" />
									</rowConstraints>
									<children>
										<Label fx:id="lLabel" text="Meine verpachteten Felder" GridPane.hgrow="NEVER" GridPane.vgrow="NEVER">
											<font>
												<Font size="24.0" />
											</font>
										</Label>
										<TableView fx:id="leasedFields" prefHeight="295.0" prefWidth="600.0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS">
											<columns>
												<TableColumn fx:id="lFieldName" prefWidth="75.0" text="Feldname" />
												<TableColumn fx:id="lStartDate" prefWidth="75.0" text="Von" />
												<TableColumn fx:id="lEndDate" prefWidth="75.0" text="Bis" />
												<TableColumn fx:id="lDetails" prefWidth="75.0" text="Details" />
											</columns>
											<columnResizePolicy>
												<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
											</columnResizePolicy>
										</TableView>
									</children>
								</GridPane>
							</children>
						</AnchorPane>
					</content>
				</Tab>
				<Tab text="Meine Daten">
					<content>
						<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
					</content>
				</Tab>
			</tabs>
		</TabPane>
	</center>
</BorderPane>
